{
  "name": "mymessenger",
  "version": "0.0.1",
  "description": "messenger on nodeJs",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets env,stage-0\"",
    "build": "babel src -s -D -d dist --presets env,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IlyaPapou/myMessenger.git"
  },
  "keywords": [
    "messenger",
    "nodeJs"
  ],
  "author": "Ilya Popov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/IlyaPapou/myMessenger/issues"
  },
  "homepage": "https://github.com/IlyaPapou/myMessenger#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.16.0",
    "nodemon": "^1.18.10"
  },
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb": "^3.2.2",
    "morgan": "^1.9.1",
    "uws": "^100.0.1"
  }
}
